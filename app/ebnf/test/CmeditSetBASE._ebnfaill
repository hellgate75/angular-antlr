"cmedit" "set" ( SET_BY_FDN | SET_BY_QUERY ) [OUTPUT-FORMAT]

SET_BY_FDN  :: = <fdn> { ";" <fdn> } ATTRIBUTE_SETTER_LIST [CONFIGURATION]

SET_BY_QUERY :: = SCOPE { ";" SCOPE } CLASS_FILTER { "," CLASS_FILTER } ATTRIBUTE_SETTER_LIST [NAMESPACE] [VERSION] [NETYPE] [CONFIGURATION]

SCOPE ::= <fdn>
          | <node-name>
          | "*"
          | ["*"] <partial-node-name> ["*"]

CLASS_FILTER :: = <mo-class-type>
                  | <mo-class-type> "." ATTRIBUTE_FILTER
                  | <mo-class-type> "." "(" ATTRIBUTE_FILTER { "," ATTRIBUTE_FILTER } ")"

ATTRIBUTE_FILTER ::= <attribute-name> COMPARISON_OPERATOR <value>

COMPARISON_OPERATOR ::= "==" | "<" | "<=" | ">" | ">="

ATTRIBUTE_SETTER_LIST :== ATTRIBUTE_SETTER { ";" | "," ATTRIBUTE_SETTER }
 
ATTRIBUTE_SETTER ::= [mo-class-type.]<attribute-name> "=" <attribute-value>
		     | [mo-class-type.]<attribute-name> "=" "(" ATTRIBUTE_SETTER_LIST ")"
		     | [mo-class-type.]<attribute-name> "=" ATTRIBUTE_ARRAY


ATTRIBUTE_ARRAY ::= "[" <attribute-value> { "," <attribute-value> } "]"
                    | "[" "(" ATTRIBUTE_SETTER_LIST ")" { "," "(" ATTRIBUTE_SETTER_LIST ")" } "]"
                     
NAMESPACE ::= ( "-ns" | "--namespace" ) "=" <namespace-name>

VERSION ::= ( "-v" | "--version" ) "=" <digit> { "." <digit> }
 
NETYPE ::= ( "-ne" | "--netype" ) "=" <netype-name>

CONFIGURATION ::= ( "-c" | "--config" ) "=" <configuration-name>

OUTPUT-FORMAT ::= ( "-l" | "-t" )
